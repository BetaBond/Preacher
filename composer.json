{
	"name": "colorful/preacher",
	"description": "Business tool library",
	"license": "mit",
	"autoload": {
		"psr-4": {
			"Colorful\\Preacher\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Colorful\\Tests\\Preacher\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "BetaBond",
			"email": "bond.ryo.beta@gmail.com"
		}
	],
	"require": {
		"php": "^8.0.2",
		"illuminate/support": "^9.0",
		"illuminate/http": "^9.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5.10"
	},
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"composer/package-versions-deprecated": true
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
